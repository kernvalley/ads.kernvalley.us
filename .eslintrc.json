{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "plugins": ["async-await"],
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "async-await/space-after-async": 2,
        "async-await/space-after-await": 2,
        "generator-star-spacing": ["error", {
            "before": false,
            "after": true,
            "anonymous": "neither",
            "method": {"before": true, "after": false}
        }]
    },
    "globals": {
        "URLSearchParams": false,
        "ga": false,
        "skipWaiting": false,
        "clients": false,
        "PasswordCredential": false
    }
}
